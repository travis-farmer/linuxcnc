    <tabs>
        <names>["Spindle", "ATC", "Laser"]</names>
        <vbox>
            <meter>
                <halpin>"spindle-rpm"</halpin>
                <text>"Spindle"</text>
                <subtext>"RPM"</subtext>
                <size>200</size>
                <min_>0</min_>
                <max_>30000</max_>
                <majorscale>2000</majorscale>
                <minorscale>1000</minorscale>
                <region1>(24000,30000,"red")</region1>
                <region2>(12000,24000,"green")</region2>
                <region3>(0,12000,"blue")</region3>
            </meter>
            <hbox>
                <label>
                        <text>"Spindle-At-Speed:"</text>
                        <font>("Helvetica",20)</font>
                </label>
                <led>
                    <halpin>"spindle-at-speed-led"</halpin>
                    <size>30</size>
                    <on_color>"green"</on_color>
                    <off_color>"red"</off_color>
                </led>
            </hbox>
            <hbox>
                <label>
                        <text>"Forward:"</text>
                        <font>("Helvetica",20)</font>
                </label>
                <led>
                    <halpin>"spindle-forward-led"</halpin>
                    <size>30</size>
                    <on_color>"green"</on_color>
                    <off_color>"red"</off_color>
                </led>
            </hbox>
            <hbox>
                <label>
                        <text>"Reverse:"</text>
                        <font>("Helvetica",20)</font>
                </label>
                <led>
                    <halpin>"spindle-reverse-led"</halpin>
                    <size>30</size>
                    <on_color>"green"</on_color>
                    <off_color>"red"</off_color>
                </led>
            </hbox>
            <label>
                    <text>"Spindle Temp (F)"</text>
                    <font>("Helvetica",14)</font>
            </label>
            <bar>
                <halpin>"spindle-temp"</halpin>
                <min_>-40</min_>
                <max_>200</max_>
                <format>"3.3f"</format>
                <bgcolor>"grey"</bgcolor>
                <fillcolor>"red"</fillcolor>
                <range1>-40,32,"blue"</range1>
                <range2>32,100,"green"</range2>
                <range3>100, 200,"red"</range3>
                <canvas_width>200</canvas_width>
                <canvas_height>50</canvas_height>
                <bar_height>30</bar_height>
                <bar_width>150</bar_width>
            </bar>
        </vbox>
        <vbox>
            <bar>
                <halpin>"comp-air"</halpin>
                <min_>0</min_>
                <max_>150</max_>
                <format>"3.1f"</format>
